{
  "name": "tpt-styleguide",
  "version": "0.0.0",
  "description": "TpT Styleguide",
  "watch": {
    "tape": "{src,tests}/*.js"
  },
  "engines": {
    "node": "4.x.x",
    "npm": "3.x.x"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean; babel src -d lib; mkdir lib/scss; for f in $(find src -name *.scss); do cp $f ${f/src/lib}; done",
    "watch": "npm-watch",
    "lint": "eslint src docs tests --ext js --ext jsx",
    "tape": "babel-tape-runner ./tests/**/*.test.jsx",
    "tape-debug": "node debug ./node_modules/babel-tape-runner/bin/babel-tape-runner ./tests/**/*.test.jsx",
    "start": "webpack-dev-server --host 0.0.0.0 --config webpack.config.js"
  },
  "license": "ISC",
  "devDependencies": {
    "@teachers/tpt-snacks": "0.0.1",
    "@teachers/tpt-ui": "0.5.16",
    "autobind-decorator": "^1.3.3",
    "autoprefixer": "6.3.6",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "codemirror": "5.12.0",
    "css-loader": "0.23.1",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-react": "4.3.0",
    "file-loader": "^0.9.0",
    "flexibility": "^1.0.5",
    "isparta-loader": "2.0.0",
    "json-loader": "0.5.4",
    "node-sass": "^3.8.0",
    "null-loader": "0.1.1",
    "postcss-flexibility": "^1.0.3",
    "postcss-loader": "^0.9.1",
    "raw-loader": "0.5.1",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-fa": "^4.1.2",
    "react-router": "2.1.1",
    "sanitize.css": "3.3.0",
    "sass-loader": "3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.5",
    "webpack-dev-server": "2.0.0-beta",
    "webpack-notifier": "1.3.0"
  }
}
